{% extends "_layouts/layout.twig" %}

{% block content %}
  <article class="mb-10 last:mb-0">
    {% include "_modules/headers/article" with {
      headline: entry.title,
      image: entry.mainImage.eagerly().one(),
      backLink: {
        text: "See all events"|t,
        url: eventsIndex.url
      },
      date: entry.date,
      times: [entry.startTime, entry.endTime]|filter(time => time is not empty),
      text: entry.location,
    } only %}
  </article>

  {% include "_modules/modules" with { entry: entry } only %}
{% endblock %}
